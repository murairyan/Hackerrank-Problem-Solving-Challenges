/*
* Complete the "countApplesAndOranges" function below.
*
* The function accepts INT s, INT t, INT a, INT b, INTEGER_ARRAY apples, INTEGER_ARRAY oranges as parameters.
*/

void countApplesAndoranges (int s, int t, int a, int b, vector<int> apples, vector<int> oranges) {
    int appleCount = 0, orangeCount = 0;
    
    // Apple Count
    for (int i = 0; i < apples.size(); it+) {
        int tempval = 0;
        tempval = a - apples[i];

        // Conditon needs to have '&&' to compare two separate values
        if(s <= tempval && tempval <= t){
            appleCount++;
        }
    }

    // Orange Count
    for (int k = 0; k < oranges.size() ; k++) {
        int tempval = 0;
        tempval = b + oranges[k];
        if(s <= tempval && tempval <= t){
            orangeCount++;
    }
}

    cout << appleCount << endl:
    cout << orangeCount << endl;
}
